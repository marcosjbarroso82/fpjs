{
    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/caloriecounter_completed_modified",
            "sourceMaps": true,
            "trace": true,
            "userDataDir": true,
            "sourceMapPathOverrides": {
                "webpack:///./~/*": "${webRoot}/node_modules/*",
                "webpack:////*": "/*",
                "webpack:///./*": "${webRoot}/*",
                "webpack:///src/*": "${webRoot}/src/*"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Vite Dev Server",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "dev"],
            "cwd": "${workspaceFolder}/caloriecounter_completed_modified",
            "sourceMaps": true,
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ],
    "compounds": [
        {
            "name": "Debug Full Stack",
            "configurations": ["Launch Vite Dev Server", "Launch Chrome against localhost"]
        }
    ]
}